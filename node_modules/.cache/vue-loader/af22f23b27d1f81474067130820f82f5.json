Entrez le nom d'une ville...</div>\n            <div v-else>\n              <span class=\"md-display-1\">{{ info.location.name }}</span>\n              <p>{{ info.location.localtime}}</p>\n              <p>{{ info.current.temperature}} °C</p>\n              <img v-bind:src=\"info.current.weather_icons\" alt=\"soviet weather logo\"/>\n              </div>\n          </section>\n        </md-card-content>\n\n        <md-card-actions>\n          <md-input v-model=\"cityName\" aria-placeholder=\"Nom de la ville\"></md-input>\n          <md-button v-on:click=\"getWeatherReport(cityName)\">Seдяch</md-button>\n        </md-card-actions>\n      </md-ripple>\n    </md-card>\n  \n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"SlavicWeather\",\n  data() {\n    return {\n      info: [],\n      errors: [],\n      cityName: null,\n      loading: true,\n      hasError: false,\n    };\n  },\n  methods:{\n    getWeatherReport: function (cityName){\n    axios\n      .get(\n        \"http://api.weatherstack.com/current?access_key=ff5c53dcb9672d68e5d347c20def0014&\", {params: {query: cityName}}\n      )\n      .then((response) => {\n        this.info = response.data;\n      })\n      .catch((e) => {\n        this.errors.push(e);\n        this.hasError = true;\n      })\n      .finally(() => (this.loading = false));\n  }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n.md-card {\n  width: 320px;\n  margin: 4px;\n  display: inline-block;\n  vertical-align: top;\n}\n</style>\n"]}]}