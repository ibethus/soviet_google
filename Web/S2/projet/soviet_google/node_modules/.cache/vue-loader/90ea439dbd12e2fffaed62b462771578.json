.</div>\n          <div v-else>\n            <span class=\"md-display-1\">{{ info.location.name }}</span>\n            <p>{{ info.location.localtime }}</p>\n            <p>{{ info.current.temperature }} °C</p>\n            <img\n              v-bind:src=\"info.current.weather_icons\"\n              alt=\"soviet weather logo\"\n            />\n          </div>\n        </section>\n      </md-card-content>\n\n      <md-card-actions>\n        <md-input v-model=\"city\" aria-placeholder=\"Nom de la ville\"></md-input>\n        <md-button v-on:click=\"getWeatherReport(city)\">Seдяch</md-button>\n      </md-card-actions>\n    </md-ripple>\n  </md-card>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"SlavicWeather\",\n  data() {\n    return {\n      info: [],\n      city: null,\n      loading: true,\n      hasError: false,\n    };\n  },\n  methods: {\n    getWeatherReport: function (city) {\n      axios\n        .get(\n          \"http://api.weatherstack.com/current?access_key=ff5c53dcb9672d68e5d347c20def0014&\",\n          { params: { query: city } }\n        )\n        .then((response) => {\n          if (response.data.success == false) {\n            throw new Error(response.data.error.info);\n          } else {\n            this.info = response.data;\n            this.hasError = false;\n            this.loading = false;\n          }\n        })\n        .catch((error) => {\n          this.hasError = true;\n          alert(\"Blyдt ! \\n\" + error);\n        });\n    },\n  },\n};\n</script>\n\n<!-- La balise scoped permet de ne charger le css que dans ce composant -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n.md-card {\n  width: 320px;\n  margin: 4px;\n  display: inline-block;\n  vertical-align: top;\n}\n</style>\n"]}]}